pipeline {
    agent {label 'JDK11' }
    triggers {
        pollSCM('* * * * *')
    stages {
        stage('clone') {
            steps {
                git url:'https://github.com/QtBhavani/shopizer.git' , branch :'master'
            }
        }
        stage('install') { 
            steps { 
                sh 'mvn package'
            }

        }
    }
}